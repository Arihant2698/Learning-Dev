const BUY_BAT="BUY_BAT";
const intialState={
    numofBats:20
}
const BatReducer= (state=intialState,action)=>{
    switch(action.type){
        case BUY_BAT:
            return{...state,numofBats:state.numofBats-action.payload}
    
        default:
            return state
        }

}
export default BatReducer